{
    "sourceMaps": true,
    "minify": false,
    "jsc": {
        "externalHelpers": true,
        "target": "es2020",
        "parser": {
            "syntax": "ecmascript",
            "jsx": true
        },
        "transform": {
            "react": {
                "development": false,
                "pragma": "React.createElement",
                "pragmaFrag": "React.Fragment"
            }
        }
    },
    "module": {
        "type": "es6"
    }
}
